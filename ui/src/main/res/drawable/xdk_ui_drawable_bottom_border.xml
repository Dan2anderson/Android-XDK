<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <stroke
                android:width="@dimen/xdk_ui_divider_thickness"
                android:color="@color/xdk_ui_color_primary_gray_3" />
            <solid android:color="@color/transparent" />
        </shape>
    </item>

    <item
        android:bottom="@dimen/xdk_ui_divider_thickness">
        <shape android:shape="rectangle">
            <stroke
                android:width="@dimen/xdk_ui_divider_thickness"
                android:color="@color/xdk_ui_window_background" />
        </shape>
    </item>

</layer-list>